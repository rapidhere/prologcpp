{%- import 'M.twig' as M -%}

template<>
struct {{ M.subFunctorId(functor) }}<
    {%- for const in args -%}
        {{ M.atom(const, context, q[loop.index0]) }},
    {%- endfor -%}
    {{- functor.getItemCount() -}}
> {
{% for const in args -%}
    {% ol tab:4 %}
    typedef pack<{{ context.getConstantIndex(const.getInt()) }}> v{{ loop.index0 }};
