{%- import 'M.twig' as M -%}

template<>
struct {{ M.subFunctorId(functor) }}<
    {%- for number in fact.getArguments() -%}
    Atom<{{ context.getConstantIndex(number.getInt()) }}>,
    {%- endfor -%}
    {{- functor.getItemCount() -}}
> {
    {% for number in fact.getArguments() -%}
        {% ol %}
        typedef pack<{{ context.getConstantIndex(number.getInt()) }}> v{{ loop.index0 }};
        {% endol %}
    {%- endfor -%}
};

